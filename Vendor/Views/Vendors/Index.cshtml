@using VendorTracker.Models;

<h1>Vendor's List</h1>

@if (@Model.Count == 0)
{
  <h3>No vendors have been added yet!</h3>
}

@foreach (Vendor vendor in Model)
{
  <h3><a href='/vendors/@vendor.Id'>  @vendor.Name</a> | @vendor.Description</h3> <!-- RESOLVED sends to vendors order list-->
}

<h5><a href='/vendor/new'>Add New Vendor</h5>  <!--RESOLVED -->
